    "lenis": "^1.3.15",
    "ogl": "^1.0.11",
    "preact": "^10.18.0",
    "preact-render-to-string": "^6.6.3",
    "react-color-palette": "^7.3.1",
    "three": "^0.180.0",
    "zod": "^4.1.11",
    "zustand": "^5.0.3"
