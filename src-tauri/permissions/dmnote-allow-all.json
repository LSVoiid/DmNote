      "commands": {
        "allow": [
          "app_bootstrap",
          "app_open_external",
          "app_restart",
          "window_minimize",
          "window_close",
          "settings_get",
          "settings_update",
          "overlay_get",
          "overlay_set_visible",
          "overlay_set_lock",
          "overlay_set_anchor",
          "overlay_resize",
          "css_get",
          "css_get_use",
          "css_toggle",
          "css_reset",
          "css_set_content",
          "css_load",
          "keys_get",
          "keys_update",
          "keys_set_mode",
          "keys_reset_all",
          "keys_reset_mode",
          "keys_reset_counters",
          "keys_reset_counters_mode",
          "positions_get",
          "positions_update",
