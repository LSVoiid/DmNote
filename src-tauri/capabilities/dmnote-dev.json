{
  "identifier": "dmnote-dev",
  "description": "Dev server capability",
  "local": true,
  "windows": ["main", "overlay"],
  "webviews": ["main", "overlay"],
  "permissions": [
    "dmnote-allow-all",
    "core:default",
    "core:window:allow-start-dragging"
  ],
  "remote": {
    "urls": [
      "http://localhost:3400/**",
      "http://127.0.0.1:3400/**",
      "http://localhost:3000/**",
      "http://127.0.0.1:3000/**",
      "tauri://localhost/**"
    ]
  }
}
