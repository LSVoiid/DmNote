{
  "identifier": "main",
  "description": "Main window capability granting DM Note access",
  "local": true,
  "windows": ["main", "overlay"],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "dmnote-allow-all"
  ],
  "remote": {
    "urls": [
      "http://127.0.0.1:3000/**",
      "http://localhost:3000/**",
      "http://127.0.0.1:3400/**",
      "http://localhost:3400/**",
      "tauri://localhost/**"
    ]
  }
}
